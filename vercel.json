{
  "version": 2,
  "builds": [
    {
      "src": "dist/app-ssr/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/app-ssr/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\..*)",
      "dest": "/dist/app-ssr/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/app-ssr/server/server.mjs"
    }
  ]
}
