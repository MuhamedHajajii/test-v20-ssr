{
  "version": 2,
  "functions": {
    "dist/app-ssr/server/server.mjs": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/dist/app-ssr/browser/favicon.ico"
    },
    {
      "src": "/(.*\\.(js|css|html|ico|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|eot|otf))",
      "dest": "/dist/app-ssr/browser/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/dist/app-ssr/server/server.mjs"
    }
  ],
  "outputDirectory": "dist/app-ssr"
}
